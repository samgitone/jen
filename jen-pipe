pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'this is build form git'
            }
        }
        stage('Test') {
            steps {
                echo 'this is test form git'
            }
        }
        stage('Deploy') {
            steps {
                echo 'this is Deploy form git'
            }
        }
    }
}
